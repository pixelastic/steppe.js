{
  "bitwise": true,
  "browser": true,
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "expr": true,
  "forin": true,
  "freeze": true,
  "funcscope": false,
  "globals": {
    "$": false,
    "Steppe": false,
    "Zepto": false,
    "_": true,
